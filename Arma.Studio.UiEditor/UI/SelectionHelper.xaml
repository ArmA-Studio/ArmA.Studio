<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                    xmlns:local="clr-namespace:Arma.Studio.UiEditor.UI"
                    xmlns:converters ="clr-namespace:Arma.Studio.UiEditor.UI.Converters"
                    xmlns:p="clr-namespace:Arma.Studio.UiEditor.Properties">
    <DataTemplate DataType="{x:Type local:SelectionHelper}">
        <Rectangle Fill="#08019CFF"
                   Stroke="#019CFF"
                   Width="{Binding Width}"
                   Height="{Binding Height}"/>
    </DataTemplate>
</ResourceDictionary>