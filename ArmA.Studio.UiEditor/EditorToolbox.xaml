<DataTemplate xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
              xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
              xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
              xmlns:local="clr-namespace:ArmA.Studio.UiEditor;assembly=ArmA.Studio.UiEditor"
              DataType="{x:Type local:EditorToolboxDataContext}"
              xmlns:ae="http://icsharpcode.net/sharpdevelop/avalonedit"
              xmlns:asd="http://schemas.armastudio.io/data"
              >
    <Grid>
        <ListBox ItemsSource="{Binding Items}" SelectedItem="{Binding SelectedItem}" asd:AttachedDataContext.DataContext="{Binding}">
            <ListBox.ItemTemplate>
                <DataTemplate DataType="{x:Type local:EditorToolboxItem}">
                    <Grid>
                        <TextBlock Text="{Binding Name}"/>
                    </Grid>
                </DataTemplate>
            </ListBox.ItemTemplate>
        </ListBox>
    </Grid>
</DataTemplate>